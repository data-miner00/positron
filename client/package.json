{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ethers": "^5.6.9",
    "framer-motion": "^6.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "6",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "sb": "yarn storybook"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
